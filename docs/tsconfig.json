{
	"extends": [
		"@tsconfig/strictest/tsconfig.json",
		"@tsconfig/node20/tsconfig.json",
		"@vue/tsconfig/tsconfig.dom.json",
	],
	"compilerOptions": {
		"types": [
			"vite/client",
		],
		"exactOptionalPropertyTypes": false,
		"resolveJsonModule": true
	},
	"include": [
		"./**/*.ts",
		"./**/*.d.ts",
		"./**/*.tsx",
		"./**/*.vue",
		".vitepress/theme/index.ts"
	],
}
